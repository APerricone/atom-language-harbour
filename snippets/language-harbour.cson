'.source.harbour':
  'if .. else .. end':
    'prefix': 'ife'
    'body': 'IF ${1:condition}\n\t$2\nELSE\n\t$3\nENDIIF'
  'if .. end':
    'prefix': 'if'
    'body': 'IF ${1:condition}\n\t$0\nENDIF'
  'case .. end':
    'prefix': 'case'
    'body': 'DO CASE'\n'CASE ${1:condition}\n$0\nCASE ${2:condition}\n$1\nOTHERWISE\n$3ENDCASE'
